[Unit]
Description=MSP API TCP broker
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
User=root
Group=root
ExecStart=/usr/bin/python3 -m mspapi2.msp_server --config /etc/mspapi2/server.json
Restart=on-failure
RestartSec=1
Environment=PYTHONUNBUFFERED=1

[Install]
WantedBy=multi-user.target
